#include <iostream>
#include <climits>
using namespace std;

int main()
{
    int maxsum=INT_MIN;
    int arr[]={-1,2,3,-4,5,-6,7,-8,9};
    int siz=9;
    for(int st=0; st<siz;st++){
        int sum=0;
        for(int j=st;j<siz-1;j++){
            
            sum+=arr[j];
            maxsum=max(sum,maxsum);
        }
    }
    cout<<maxsum<<endl;
    return 0;
}
