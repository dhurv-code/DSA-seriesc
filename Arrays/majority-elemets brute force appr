class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        for(int val: nums){
            int add=0;
            for(int count: nums){
                if (val==count){
                    add++;
                }
            }
        if(add>n/2){
            return val;
        }
        }
        return -1;

    }
};
